  "keywords": [
    "angular 5",
    "game",
    "the interview"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:aot": "ng build --prod --aot",
    "debug-test": "ng test",
    "test": "ng test --code-coverage --watch false --reporters junit,coverage-istanbul",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e",
    "highload": "artillery -o reports/highload/highload run highload.json && artillery report reports/highload/highload",
    "doc": "./node_modules/.bin/compodoc -p tsconfig.json --hideGenerator --theme postmark",
    "doc:live": "compodoc -p tsconfig.json -s --hideGenerator --theme postmark -w -o"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "intl": "^1.2.5",
    "rxjs": "^5.5.2",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.5.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
